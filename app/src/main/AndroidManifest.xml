<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mcas2.interfacedesign">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_boot_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_boot_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InterfaceDesign">
        <activity
            android:name=".MainBN"
            android:exported="false"
            android:label="@string/title_activity_main_bn"
            android:theme="@style/Theme.InterfaceDesign.NoActionBar" />
        <activity
            android:name=".MainBAB"
            android:exported="false" />
        <activity
            android:name=".SignUp"
            android:exported="true"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".Splash_Screen"
            android:exported="true"
            android:theme="@style/NoActionTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".LoginActivity"
            android:exported="true"
            android:theme="@style/NoActionTheme" />
    </application>

</manifest>